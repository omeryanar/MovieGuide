<MudStack Class="mt-4" Spacing="2" Justify="Justify.SpaceBetween">
    @if(Networks != null)
    {
        foreach(Network network in Networks)
        {
            <MudLink Href="@($"discover/tv?with_networks={network.Id}")">
                <MudImage Width="200" Src="@network.LogoFullPath" />
            </MudLink>
        }
    }
</MudStack>

@code
{
    [Parameter]
    public List<Network> Networks { get; set; }
}
